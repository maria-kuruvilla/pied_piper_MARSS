@article{arevalo_does_2021,
	title = {Does global change increase the risk of maladaptation of {Atlantic} salmon migration through joint modifications of river temperature and discharge?},
	volume = {288},
	issn = {0962-8452, 1471-2954},
	url = {https://royalsocietypublishing.org/doi/10.1098/rspb.2021.1882},
	doi = {10.1098/rspb.2021.1882},
	abstract = {In freshwater ecosystems, water temperature and discharge are two intrinsically associated triggers of key events in the life cycle of aquatic organisms such as the migration of diadromous fishes. However, global changes have already profoundly altered the thermal and hydrological regimes of rivers, affecting the timing of fish migration as well as the environmental conditions under which it occurs. In this study, we focused on Atlantic salmon (
              Salmo salar
              ), an iconic diadromous species whose individuals migrate between marine nursery areas and continental spawning grounds. An innovative multivariate method was developed to analyse long-term datasets of daily water temperature, discharge and both salmon juvenile downstream and adult upstream migrations in three French rivers (the Bresle, Oir and Nivelle rivers). While all three rivers have gradually warmed over the last 35 years, changes in discharge have been very heterogeneous. Juveniles more frequently used warmer temperatures to migrate. Adults migrating a few weeks before spawning more frequently used warm temperatures associated with high discharges. This has already led to modifications in preferential niches of both life stages and suggests a potential mismatch between these populations' ecological preference and changes in their local environment due to global change.},
	language = {en},
	number = {1964},
	urldate = {2023-01-06},
	journal = {Proceedings of the Royal Society B: Biological Sciences},
	author = {Arevalo, Elorri and Maire, Anthony and Tétard, Stéphane and Prévost, Etienne and Lange, Frédéric and Marchand, Frédéric and Josset, Quentin and Drouineau, Hilaire},
	month = dec,
	year = {2021},
	pages = {20211882},
	annote = {For atlantic salmon, earlier spring migration comes with higher risk associated with spring flooding and greater energy loss. Late summer or autumn migration are likely to face higher temperatures near their thermal tolerance and lower water levels. 
available niche - environmental conditions available
preferential niche - environmental conditions selected by species
effective niche - interaction between available and preferential niches used by the species.

In this paper they track changes in available niche, changes in effective niche, and discuss potential implication of niche shift relative to preferential niche.

Choc analysis - 
daily proportion of smolts  = daily number/ total number of smolts that year
warm water was used more frequently by smolts to migrate
highest disharges were used more frequently by smolts
colder water discharges has become more scarce and warm water discharges have been more frequent recently
studies have shown that smolts respond to environemntal cues to migrate and reach the ocean when water temperatures are above 8 degree C to improve feeding conditions, salinity tolerance and swimming performance. migrating in the summer causes more energy loss. earlier migration of smolts that are smaller in size makes them more vulnerable to predation. 

Preferential niche is evolving towards warmer waters


Important references - “Hvidsten NA, Jensen AJ, Vivas H, Bakke O, Heggberget TG. 1995 Downstream migration of Atlantic salmon smolts in relation to water flow, water temperature, moon phase and social interaction. Nord. J. Freshw. Res. 70,38–48. 61. 
Antonsson T, Gudjonsson S. 2002 Variability in timing and characteristics of Atlantic salmon smolt in Icelandic rivers. Trans. Am. Fish. Soc. 131, 643–655. (doi:10.1577/1548-8659(2002)131) 62. 
Jutila E, Jokikokko E, Julkunen M. 2005 The smolt run and postsmolt survival of Atlantic salmon, Salmo salar L., in relation to early summer water temperatures in the northern Baltic Sea. Ecol. Freshw. Fish 14,69–78. (doi:10.1111/j.1600-0633. 2005.00079.x) 63. 
Jonsson B, Jonsson N. 2009 A review of the likely effects of climate change on anadromous Atlantic salmon Salmo salar and brown trout Salmo trutta, with particular reference to water temperature and flow. J. Fish Biol. 75, 2381–2447. (doi:10.1111/j. 1095-8649.2009.02380.x)” (Arevalo et al., 2021, p. 10)



},
	file = {Arevalo et al. - 2021 - Does global change increase the risk of maladaptat.pdf:C\:\\Users\\maria\\Zotero\\storage\\IYFSSTV5\\Arevalo et al. - 2021 - Does global change increase the risk of maladaptat.pdf:application/pdf},
}

@article{banerjee_using_2019,
	title = {Using machine learning to assess short term causal dependence and infer network links},
	volume = {29},
	issn = {1054-1500, 1089-7682},
	url = {http://aip.scitation.org/doi/10.1063/1.5134845},
	doi = {10.1063/1.5134845},
	abstract = {We introduce and test a general machine-learning-based technique for the inference of short term causal dependence between state variables of an unknown dynamical system from time-series measurements of its state variables. Our technique leverages the results of a machine learning process for short time prediction to achieve our goal. The basic idea is to use the machine learning to estimate the elements of the Jacobian matrix of the dynamical ow along an orbit. The type of machine learning that we employ is reservoir computing. We present numerical tests on link inference of a network of interacting dynamical nodes. It is seen that dynamical noise can greatly enhance the e ectiveness of our technique, while observational noise degrades the e ectiveness. We believe that the competition between these two opposing types of noise will be the key factor determining the success of causal inference in many of the most important application situations.},
	language = {en},
	number = {12},
	urldate = {2023-01-06},
	journal = {Chaos: An Interdisciplinary Journal of Nonlinear Science},
	author = {Banerjee, Amitava and Pathak, Jaideep and Roy, Rajarshi and Restrepo, Juan G. and Ott, Edward},
	month = dec,
	year = {2019},
	pages = {121104},
	annote = {Consider an unknown dynamical system with state variables with time series measurements of the state variables. This paper tests a machine learning technique to determine causal dependence.
two types of error - observational noise and dynamical noise. reservoir computing works best for dynamical noise and not observational noise. 
determine whether a state variable causally influences another state variable in a time evolving dynamical system (classification ML)
determine the strength of this causal influence (regression ML)
will a perturbation applied at time t to Z\_i cause a subsequent change to Z\_j at a later time t + tau 
Assumption - dz/dt = F(z)
f\_ji = {\textless}G(del F\_j/del z\_i){\textgreater}
},
	file = {Banerjee et al. - 2019 - Using machine learning to assess short term causal.pdf:C\:\\Users\\maria\\Zotero\\storage\\UYXIF2L6\\Banerjee et al. - 2019 - Using machine learning to assess short term causal.pdf:application/pdf;Submitted Version:C\:\\Users\\maria\\Zotero\\storage\\7GT8LJZ4\\Banerjee et al. - 2019 - Using machine learning to assess short term causal.pdf:application/pdf},
}

@article{roper_emigration_1999,
	title = {Emigration of age-0 chinook salmon ({Oncorhynchus} tshawytscha) smolts from the upper {South} {Umpqua} {River} basin, {Oregon}, {U}.{S}.{A}.},
	volume = {56},
	author = {Roper, Brett B. and Scarnecchia, Dennis L.},
	abstract = {Two rotating smolt traps were used through 4 consecutive years to monitor emigrations of age-0 chinook salmon (Oncorhynchus tshawytscha) from two watersheds of the upper South Umpqua River basin, Oregon, U.S.A. The number of wild smolts moving past the mainstem South Umpqua River trap ranged from 26 455 in 1991 to less than 5000 in 1993. The number of wild smolts passing the Jackson Creek trap ranged from 13 345 in 1991 to 0 in 1993. Higher numbers of wild smolts were significantly (P = 0.003) correlated with higher numbers of prespawning adults counted in index reaches the preceding year. Timing of emigration of smolts was found to be significantly related to stream temperature (P {\textless} 0.05) and phase of the lunar cycle (P {\textless} 0.05) but not related to changes in discharge (P {\textgreater} 0.05). Median emigration dates, which varied over 9 weeks, were earlier when spring water temperatures were higher. On average, two thirds of yearly smolt runs occurred when the moon was either waning or new, even though these moon phases were present only about half of the time. Significantly (P {\textless} 0.05) more fish than expected emigrated past both traps when day length was increasing.},
	language = {en},
	year = {1999},
	annote = {They looked at downstream migration of age 0 chinook in two creeks in Oregon. The numbers have declined drastically from 1991 to 1993 which correlated with adults counted in the previous years. Timing of smolts were correlated with stream temperature and lunar phase but not changes in discharge. 
},
	file = {1999 - Jcjfascjfas56CJFAS-06F99-023.vp.pdf:C\:\\Users\\maria\\Zotero\\storage\\L499FLLA\\1999 - Jcjfascjfas56CJFAS-06F99-023.vp.pdf:application/pdf},
}

@article{pavlov_downstream_2017,
	title = {Downstream migration and mechanisms of dispersal of young fish in rivers},
	volume = {74},
	issn = {0706-652X, 1205-7533},
	url = {http://www.nrcresearchpress.com/doi/10.1139/cjfas-2016-0298},
	doi = {10.1139/cjfas-2016-0298},
	abstract = {Downstream migration (DSM) of young sh has traditionally been considered a passive process. A review of studies of the patterns and mechanisms of DSM showed that it met the main criterion for migration: that animals move between well-dened habitats according to a predictable schedule. During DSM, sh often leave the habitat in which they reside (inshore zone) and enter a migratory habitat (main channel). DSM is manifested at a seasonal or diel scale and in species-specic spatial distribution patterns. According to our “dispersal with low dilution” hypothesis, the supposed adaptive value of DSM is due not only to dispersal but also to maintenance of aggregations and synchronization of migratory activity. Temporal and spatial peaks of migration promote efcient intraspecic interactions, such as shoaling, and the integrity of populations. We conclude that migratory behaviour is most successful in habitats with optimum complexity and that the inuence of behaviour on DSM is most important at key periods and sites (twilight windows and interfaces between habitats).},
	language = {en},
	number = {8},
	urldate = {2023-01-07},
	journal = {Canadian Journal of Fisheries and Aquatic Sciences},
	author = {Pavlov, Dmitrii S. and Mikheev, Victor N.},
	month = aug,
	year = {2017},
	pages = {1312--1323},
	file = {Pavlov and Mikheev - 2017 - Downstream migration and mechanisms of dispersal o.pdf:C\:\\Users\\maria\\Zotero\\storage\\RDCURA4S\\Pavlov and Mikheev - 2017 - Downstream migration and mechanisms of dispersal o.pdf:application/pdf},
