@article{berdahl_social_2017,
	title = {Social interactions shape the timing of spawning migrations in an anadromous fish},
	volume = {126},
	issn = {0003-3472},
	url = {https://www.sciencedirect.com/science/article/pii/S0003347217300295},
	doi = {10.1016/j.anbehav.2017.01.020},
	abstract = {Mass migrations are found throughout the animal kingdom and are often undertaken by coordinated social groups. However, surprisingly little is known about how social interactions influence migratory timing. Anadromous fishes such as salmon make extensive breeding migrations between marine and freshwater ecosystems. Returning adult salmon tend to move in discrete temporal pulses, which are typically thought to be triggered by abiotic environmental stimuli (e.g. changes in river flow or temperature). However, most studies reveal only weak correlations between abiotic factors and the timing of spawning runs. Here, we demonstrate that social interactions provide a plausible alternative or additional explanation for such patterns. We first provide an example of the phenomenon using 20 years of data on sockeye salmon, Oncorhynchus nerka, ascending a stream in pulses in the absence of any obvious environmental triggers. Next, we present a model that reproduces the pulses observed in the data, simply by including social interactions among individuals. Deviations between the empirical data and the social model results suggest that salmon may alter their individual behaviour in response to annual fluctuations in density. We hope our results, demonstrating the role that social influence can play on migration timing, will motivate further studies exploring how social interactions may shape the movements of other migratory taxa. Understanding how individuals integrate social information with internal and exogenous drivers of migratory behaviour is vital, particularly in the face of a changing climate, which is changing both the cues used for, and the optimal timing of, migrations.},
	language = {en},
	urldate = {2023-01-20},
	journal = {Animal Behaviour},
	author = {Berdahl, Andrew and Westley, Peter A. H. and Quinn, Thomas P.},
	month = apr,
	year = {2017},
	keywords = {collective behaviour, migration timing, salmon, social interaction, synchronization},
	pages = {221--229},
	annote = {They argue that since the environmental factors alone are not enough to model salmon migration timing, and that salmon are seen moving in discrete pulses, social influence might be playing an important role. 

They model the probability to arrive for any given fish. This can be thought of as the expected number of fish to arrive on any given day.

They used cumulative counts of salmon because data had missing days where counts did not occur. The subsequent days counted all fish until that point (but would that cause pulses in the data even if there were no pulses of salmon coming?)

They calculated the day at which 50\% of the salmon had arrived, tau.

They set tau to be 200.
They found mean cumulative distribution follows normal distribution


},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\maria\\Zotero\\storage\\RK9WYZGP\\Berdahl et al. - 2017 - Social interactions shape the timing of spawning m.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\maria\\Zotero\\storage\\HMTUJR53\\S0003347217300295.html:text/html},
