	journal = {Canadian Journal of Fisheries and Aquatic Sciences},
	author = {Spence, Brian C. and Dick, E.J.},
	editor = {Fleming, Ian},
	month = jan,
	year = {2014},
	pages = {56--69},
	annote = {If salmon are timing their migration to the ocean to enter the ocean at favourable conditions, there should be geographic variation in the influence of environmental factors that affect migration timing of smolts. 
They looked at only Coho
In Oregon, they found that photoperiod, temperature (absolute and change), flow (absolute and change) and lunar phase were significant predictors of migration probability with some factors interacting. - climate change could lead to advancement in migration timing. climate change might delay coastal upwelling according to predictions. again mismatch
In BC, all the above factors except lunar phase were significant in different ways. 
In Alaska, they found that only photoperiod (dominant) and temperature  and interaction between these terms were significant - suggesting more responsive to changes in the marine environment. modest climate change will cause mismatch

“Although our analysis includes only one or two populations in each of the oceanic domains, the regional consistencies in migration patterns exhibited by many coho populations (Spence and Hall 2010) suggest that the population-specific responses we observed may occur across larger spatial scales as well” (Spence and Dick, 2014, p. 67)
},
	file = {Spence and Dick - 2014 - Geographic variation in environmental factors regu.pdf:C\:\\Users\\maria\\Zotero\\storage\\7LIMHL6U\\Spence and Dick - 2014 - Geographic variation in environmental factors regu.pdf:application/pdf},
}

@article{bilton_influence_1982,
