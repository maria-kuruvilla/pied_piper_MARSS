@article{noauthor_emigration_1999,
	title = {Emigration of age-0 chinook salmon ({Oncorhynchus} tshawytscha) smolts from the upper {South} {Umpqua} {River} basin, {Oregon}, {U}.{S}.{A}.},
	volume = {56},
	abstract = {Two rotating smolt traps were used through 4 consecutive years to monitor emigrations of age-0 chinook salmon (Oncorhynchus tshawytscha) from two watersheds of the upper South Umpqua River basin, Oregon, U.S.A. The number of wild smolts moving past the mainstem South Umpqua River trap ranged from 26 455 in 1991 to less than 5000 in 1993. The number of wild smolts passing the Jackson Creek trap ranged from 13 345 in 1991 to 0 in 1993. Higher numbers of wild smolts were significantly (P = 0.003) correlated with higher numbers of prespawning adults counted in index reaches the preceding year. Timing of emigration of smolts was found to be significantly related to stream temperature (P {\textless} 0.05) and phase of the lunar cycle (P {\textless} 0.05) but not related to changes in discharge (P {\textgreater} 0.05). Median emigration dates, which varied over 9 weeks, were earlier when spring water temperatures were higher. On average, two thirds of yearly smolt runs occurred when the moon was either waning or new, even though these moon phases were present only about half of the time. Significantly (P {\textless} 0.05) more fish than expected emigrated past both traps when day length was increasing.},
	language = {en},
	year = {1999},
	annote = {They looked at downstream migration of age 0 chinook in two creeks in Oregon. The numbers have declined drastically from 1991 to 1993 which correlated with adults counted in the previous years. Timing of smolts were correlated with stream temperature and lunar phase but not changes in discharge. 
},
	file = {1999 - Jcjfascjfas56CJFAS-06F99-023.vp.pdf:C\:\\Users\\maria\\Zotero\\storage\\L499FLLA\\1999 - Jcjfascjfas56CJFAS-06F99-023.vp.pdf:application/pdf},
}

@article{pavlov_downstream_2017,
	title = {Downstream migration and mechanisms of dispersal of young fish in rivers},
	volume = {74},
	issn = {0706-652X, 1205-7533},
	url = {http://www.nrcresearchpress.com/doi/10.1139/cjfas-2016-0298},
	doi = {10.1139/cjfas-2016-0298},
	abstract = {Downstream migration (DSM) of young sh has traditionally been considered a passive process. A review of studies of the patterns and mechanisms of DSM showed that it met the main criterion for migration: that animals move between well-dened habitats according to a predictable schedule. During DSM, sh often leave the habitat in which they reside (inshore zone) and enter a migratory habitat (main channel). DSM is manifested at a seasonal or diel scale and in species-specic spatial distribution patterns. According to our “dispersal with low dilution” hypothesis, the supposed adaptive value of DSM is due not only to dispersal but also to maintenance of aggregations and synchronization of migratory activity. Temporal and spatial peaks of migration promote efcient intraspecic interactions, such as shoaling, and the integrity of populations. We conclude that migratory behaviour is most successful in habitats with optimum complexity and that the inuence of behaviour on DSM is most important at key periods and sites (twilight windows and interfaces between habitats).},
	language = {en},
	number = {8},
	urldate = {2023-01-07},
	journal = {Canadian Journal of Fisheries and Aquatic Sciences},
	author = {Pavlov, Dmitrii S. and Mikheev, Victor N.},
	month = aug,
	year = {2017},
	pages = {1312--1323},
	file = {Pavlov and Mikheev - 2017 - Downstream migration and mechanisms of dispersal o.pdf:C\:\\Users\\maria\\Zotero\\storage\\RDCURA4S\\Pavlov and Mikheev - 2017 - Downstream migration and mechanisms of dispersal o.pdf:application/pdf},
